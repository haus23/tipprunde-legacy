{
  "name": "@haus23/tipprunde-model",
  "description": "Domain-Modell der Haus23 Tipprunde",
  "version": "0.21.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "test": "vitest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "valibot": "1.1.0"
  },
  "peerDependencies": {
    "valibot": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "valibot": {
      "optional": true
    }
  },
  "devDependencies": {
    "@haus23/tipprunde-tsconfig": "workspace:^",
    "tsup": "^8.4.0",
    "typescript": "~5.8.3",
    "vitest": "^3.1.1"
  }
}
